<h1>Notes</h1>

<table>
  <thead>
    <tr>
      <th>Time</th>
      <th>Note</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @notes.each do |note| %>
    <tr>
    <iframe 
              width="854" 
              height="480" 
              src="https://www.youtube.com/embed/<%= note.video.video.to_s %>?modestbranding=1" 
              frameborder="0" 
              allowfullscreen></iframe>              
          </div>
    </tr>
      <tr>
        <td><%= note.tempo %></td>
        <td><%= note.note %></td>
        <td><%= link_to 'Show', note %></td>
        <td><%= link_to 'Edit', edit_note_path(note) %></td>
        <td><%= link_to 'Destroy', note, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Note', new_note_path %>
